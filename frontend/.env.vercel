# Variables d'environnement pour Vercel
# Copiez ce fichier et configurez ces variables dans votre dashboard Vercel

# URL de l'API backend sur Render
REACT_APP_API_URL=https://votre-backend.onrender.com/api

# Configuration de l'application
REACT_APP_APP_NAME=LikaFood
REACT_APP_VERSION=1.0.0

# Configuration PWA
REACT_APP_ENABLE_PWA=true

# Configuration de d√©veloppement (optionnel)
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Configuration des notifications (optionnel)
REACT_APP_ENABLE_NOTIFICATIONS=true

# Configuration de l'authentification
REACT_APP_JWT_EXPIRY=24h

# Configuration de l'interface
REACT_APP_DEFAULT_LANGUAGE=fr
REACT_APP_THEME=light