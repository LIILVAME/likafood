{
  "name": "likafood-backend",
  "version": "1.0.0",
  "description": "Backend API for LikaFood MVP - Food Vendor Management System",
  "main": "server.js",
  "scripts": {
    "start": "PORT=5001 node server.js",
    "dev": "PORT=5001 nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "node test-auth.js",
    "test:dishes": "node test-dishes.js",
    "test:comprehensive": "node comprehensive-test.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit --audit-level moderate",
    "lint": "echo \"Linting not configured yet\"",
    "validate-env": "node -e \"require('dotenv').config(); console.log('Environment variables loaded successfully')\""
  },
  "keywords": [
    "food",
    "vendor",
    "api",
    "express",
    "mongodb"
  ],
  "author": "LikaFood Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "redis": "^4.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.7.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.1",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
